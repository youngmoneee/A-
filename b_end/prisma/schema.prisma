generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum OauthProvider {
	LOCAL
	KAKAO
	GOOGLE
}

enum ROLE {
	ADMIN
	USER
}

model User {
  id		Int	@default(autoincrement())	@id
  provider	OauthProvider
  userId	String
  userName	String
  userEmail	String?
  userImage	String?
  userRole	ROLE?
}
